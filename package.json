{
	"name": "docker-dashboard-app",
	"version": "1.1.8",
	"description": "Node package that creates a dashboard UI for your Docker environment",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "parcel ./src/index.html",
		"build": "node node_modules/parcel-bundler/.bin/parcel build ./src/index.html",
		"precommit": "npm run format",
		"format": "prettier --config .prettierrc --write \"./src/**/*.js\"",
		"postinstall": "npm run build"
	},
	"bin": {
		"docker-dashboard": "./server.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/aukai84/docker-dashboard.git"
	},
	"keywords": ["Node", "Docker", "React", "Express", "Material Design Lite", "Material Design"],
	"author": "Aukai Tirrell",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/aukai84/docker-dashboard/issues"
	},
	"homepage": "https://github.com/aukai84/docker-dashboard#readme",
	"dependencies": {
		"@material-ui/core": "^3.1.0",
		"@material-ui/icons": "^3.0.1",
		"axios": "^0.18.0",
		"chart.js": "^2.7.2",
		"classes": "^0.3.0",
		"classnames": "^2.2.6",
		"cors": "^2.8.4",
		"dockerode": "^2.5.7",
		"dotenv": "^6.0.0",
		"express": "^4.16.3",
		"font-awesome": "^4.7.0",
